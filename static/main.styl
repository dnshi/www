// Override semantic ui styles
.ui.grid
  margin 0

.at.icon
  margin-right 0 !important

.ui.progress
  background rgba(0, 0, 0, .17)
  .bar > .progress
    color rgba(50, 50, 50, .8)
  & > .label
    color #38BD75 !important
    font-size 1.1em

span.angle.down
  position absolute
  bottom 5px
  margin-left -25px

// Tags
main
  height 100%
  text-shadow 0 1px 3px rgba(0,0,0,.5)

aside
  min-height 100%
  display flex !important
  justify-content center
  align-items center
  background url("data:image/jpeg;base64,")
  background-size cover

section
  min-height 100%
  background-color rgb(58, 60, 64)
  position absolute !important
  right 0
  // optional background:
  // background url(/static/dark_embroidery.png)
  background url("data:image/png;base64,")
  opacity 0.96
  box-shadow: inset 0 0px 30px black

footer
  color white
  font-size 15px
  position absolute !important
  bottom 0

// Classes
.ui.header
  color white

.ui.header .sub.header
  color rgba(255,255,255,.7)

.ui.list
  .social > .item
    margin 0
    padding-left 5px
    padding-right 5px

  .item a
    color rgba(255, 255, 255, 0.9)!important
    &:hover
      color white !important

.basic.segment
  .header, .label
    color white
    font-weight 500

  & > .header
    color rgb(127, 222, 228)

  .header .sub.header
    margin-top 10px
    color rgb(168, 173, 179)

  .content > .header
    padding-left 15px

.EOF > h2
  text-align center

// Responsive:PC
@media only screen and (min-width 768px)
  aside, section
    position absolute !important
    top 0
    bottom 0
    overflow auto

  aside, footer
    max-width 420px !important

  section
    right 0
    padding-top 25px !important
    padding-left 25px !important
    padding-right 25px !important

  span.angle.down
    display none

  .basic.segment > .header
    padding-left 1rem
    padding-right 1rem


@media only screen and (min-width 1120px)
  .ui.column.grid>[class*="ten wide"].column
    left 420px
    width auto !important


@media only screen and (min-width 1420px)
  .ui.basic.segment
    width 889px
    margin 5% auto 0 auto

// Respsonsive:Mobile
@media only screen and (max-width 767px)
  aside.center.aligned.six.wide.column
    display flex !important
    box-shadow 0 5px 15px black !important
    z-index 1

// Animation
.pulsate-opacity
  animation pulsate-op 2.3s infinite

@keyframes pulsate-op
  0%
    opacity 0.2

  50%
    opacity 1

  100%
    opacity 0.2
